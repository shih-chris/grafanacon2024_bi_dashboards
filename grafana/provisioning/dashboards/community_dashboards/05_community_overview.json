          "rawQuery": true,
          "rawSql": "with\n\npage_views as (\n\n    select\n        date_trunc('${time_period}', p.received_date) as time_period\n        ,sum(page_views) as page_views\n        ,sum(new_page_views) as new_page_views\n        ,sum(unique_visitors) as unique_visitors\n        ,sum(unique_pages) as unique_pages\n    from\n        rpt_community_stats_daily p\n    group by 1\n\n),\n\nfinal as (\n\n    select\n        p.time_period\n        ,p.page_views\n        ,p.new_page_views\n        ,p.unique_visitors\n        ,p.unique_pages\n        ,avg(p.page_views) over (order by p.time_period rows between (${moving_average_periods}-1) preceding and current row) as moving_average\n    from\n        page_views p\n\n)\n\nselect *\nfrom final\nwhere \n    $__timeFilter(time_period)\n    and time_period between '2023-04-07' and '2024-03-28'\norder by 1\n",
